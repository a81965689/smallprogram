page{
  background-color: #333;
}
.nav{
  width: 96%;
  padding-top: 160rpx;
  margin: 0 auto;
}
.nav>navigator{
  width: 100%;
  height: 200rpx;
  line-height: 200rpx;
  text-align: center;
  font-size: 40rpx;
  font-weight: bold;
  color: #fff;
  border-radius: 20rpx;
  margin-bottom: 48rpx;
}
.nav>navigator:nth-child(1){
   background: linear-gradient(60deg, #E66B5F , #E9557E);
}
.nav>navigator:nth-child(2){
   background: linear-gradient(60deg, #F8B35C , #F88E7A);
}
.nav>navigator:nth-child(3){
   background: linear-gradient(60deg, #66CBA0 , #66C5C0);
}